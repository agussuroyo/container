{
    "name": "agussuroyo/container",
    "description": "Simple dependency injection container",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Agus Suroyo"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0 || ^11.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AgusSuroyo\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgusSuroyo\\Container\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src tests --level=max",
        "check": [
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
